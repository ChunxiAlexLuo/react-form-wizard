apiVersion: app.k8s.io/v1beta1
kind: Application
metadata:
  name: {{name}}
  namespace: {{namespace}}
spec:
  componentKinds:
  - group: apps.open-cluster-management.io
    kind: Subscription
  descriptor: {}
{{#if name}}
  selector:
    matchExpressions:
      - key: app
        operator: In
        values: 
          - {{name}}
{{/if}}
{{#each repositories}}
---
kind: Application
spec:
  - type: {{type}}
---
kind: Application
{{/each}}